{
  "name": "@pxblue/react-components",
  "version": "0.0.1",
  "description": "React components for PX Blue applications",
  "scripts": {
    "test": "cd components && yarn test",
    "build": "cd components && yarn && yarn build",
    "link:components": "yarn link:components:mac",
    "link:components:mac": "./linkComponents.sh",
    "link:components:win": "\"%ProgramFiles%/Git/bin/bash.exe\" ./linkComponents.sh",
    "start": "cd component-demo && yarn && cd .. && yarn link:components && cd component-demo && yarn start",
    "start:win": "cd component-demo && yarn && cd .. && yarn link:components:win && cd component-demo && yarn start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pxblue/component-library.git"
  },
  "keywords": [
    "react",
    "components",
    "pxblue"
  ],
  "author": "pxblue <pxblue@eaton.com>",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/pxblue/component-library/issues"
  },
  "homepage": "https://github.com/pxblue/component-library#readme",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "dependencies": {
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "jest": "^24.8.0"
  }
}
