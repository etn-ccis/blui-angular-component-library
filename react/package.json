{
  "name": "@pxblue/react-components",
  "version": "1.2.0",
  "description": "React components for PX Blue applications",
  "scripts": {
    "test": "cd components && yarn test",
    "test:artifacts": "bash ./buildTest.sh",
    "build": "cd components && yarn && yarn build",
    "link:components": "bash ./linkComponents.sh",
    "install:dependencies": "cd component-demo/showcase && yarn && cd ../storybook && yarn && cd ../../components && yarn",
    "start:showcase": "yarn install:dependencies && yarn link:components && cd component-demo/showcase && yarn start",
    "start:storybook": "yarn install:dependencies && yarn link:components && cd component-demo/storybook && yarn start",
    "start": "yarn start:showcase",
    "prettier": "prettier \"components/src/Drawer/**/**.{ts,tsx,js,jsx,json,css,scss,html}\" --write"
  },
  "prettier": "@pxblue/prettier-config",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pxblue/component-library.git"
  },
  "keywords": [
    "react",
    "components",
    "pxblue"
  ],
  "author": "pxblue <pxblue@eaton.com>",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/pxblue/component-library/issues"
  },
  "homepage": "https://github.com/pxblue/component-library#readme",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "dependencies": {
    "@storybook/addon-info": "^5.2.8",
    "react-router-dom": "^5.1.2"
  },
  "devDependencies": {
    "@pxblue/prettier-config": "^1.0.1",
    "node-sass": "^4.13.0",
    "prettier": "^1.19.1"
  },
  "files": [
    "package.json",
    "LICENSE",
    "README.md",
    "CHANGELOG.md",
    "core/"
  ]
}
