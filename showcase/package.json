{
  "name": "angular-showcase-demo",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:lib": "ng serve --c lib --open --host 0.0.0.0",
    "build": "ng build",
    "test": "ng test",
    "test:ci": "ng test --watch=false",
    "lint": "eslint \"src/**/**.ts\"",
    "lint:fix": "eslint \"src/**/**.ts\" --fix",
    "prettier": "prettier \"src/**/**.{ts,json,css,scss,html}\" --write",
    "prettier:check": "prettier \"src/**/**.{ts,json,css,scss,html}\" --check",
    "precommit": "yarn && yarn prettier && yarn lint && yarn test",
    "coverage": "yarn test --no-watch --code-coverage",
    "npm-check-updates": "npm-check-updates --target minor -u"
  },
  "prettier": "@brightlayer-ui/prettier-config",
  "private": true,
  "dependencies": {
