    "@angular/language-service": "^15.2.0",
    "@brightlayer-ui/eslint-config": "^3.0.1",
    "@brightlayer-ui/prettier-config": "^1.0.3",
    "@types/jasmine": "~3.10.6",
    "@types/jasminewd2": "~2.0.10",
