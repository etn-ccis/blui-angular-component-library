{
    "name": "@pxblue/angular-components",
    "version": "0.0.1",
    "description": "Angular components for PX Blue applications",
    "main": "./dist",
    "scripts": {
        "test": "cd ./components && ng test --watch=false",
        "test:artifacts": "bash ./buildTest.sh",
        "build": "./buildComponents.sh ",
        "link:components": "yarn link:components:mac",
        "link:components:mac": "./linkComponents.sh",
        "link:components:win": "bash ./linkComponents.sh",
        "start": "cd component-demo && yarn && cd .. && yarn link:components && cd component-demo && yarn start --open",
        "start:win": "cd component-demo && yarn && cd .. && yarn link:components:win && cd component-demo && yarn start --open"
    },
    "private": true,
    "peerDependencies": {
        "@angular/material": "7.2.0",
        "@angular/cdk": "^7.2.0",
        "@pxblue/themes": "^2.0.2",
        "@pxblue/colors": "^1.0.13"
    },
